{
  "name": "court-booker-root",
  "version": "1.0.0",
  "private": true,
  "description": "Court Booker - Automated amenity reservation system",
  "packageManager": "pnpm@10.6.0",
  "scripts": {
    "check": "pnpm --filter @court-booker/shared build && ts-node -r tsconfig-paths/register scripts/check-now.ts",
    "build": "pnpm --filter @court-booker/shared build",
    "build:all": "pnpm -r build"
  },
  "dependencies": {
    "@court-booker/shared": "workspace:*",
    "dotenv": "^17.2.2"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
